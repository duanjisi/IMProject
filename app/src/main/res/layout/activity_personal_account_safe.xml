<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color">

    <include layout="@layout/common_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.5dp"
        android:background="@color/line_color_gray_d2"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_app_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/app_num"/>

            <TextView
                android:id="@+id/tv_app_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/text_app_num_gray"
                android:text="666666"
                android:textSize="15.4sp"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="22dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:background="@color/background_color"/>

        <RelativeLayout
            android:id="@+id/rl_qq_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_bt_selector"
            android:layout_marginBottom="1dp"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/qq_num"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/hp_some_go"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_phone_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_bt_selector"
            android:layout_marginBottom="1dp"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/phone_num"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/hp_some_go"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_email_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_bt_selector"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/email_address"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/hp_some_go"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="22dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:background="@color/background_color"/>

        <RelativeLayout
            android:id="@+id/rl_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_bt_selector"
            android:layout_marginBottom="1dp"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/password"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/hp_some_go"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_account_protect"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_bt_selector"
            android:layout_marginBottom="1dp"
            android:paddingRight="16.5dp"
            android:paddingLeft="16.5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/iv_photo"
                android:layout_centerVertical="true"
                android:layout_marginRight="16.5dp"
                android:textSize="17.5sp"
                android:textColor="@color/black"
                android:text="@string/account_protect"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/hp_some_go"/>

        </RelativeLayout>

    </LinearLayout>

</LinearLayout>